{
  "name": "@reduct/assembler",
  "version": "0.1.2",
  "description": "Parses a DOM Node for tags and executes the matching Constructor on each element.",
  "main": "Dist/Assembler.js",
  "scripts": {
    "test": "TEST=true mocha --compilers js:babel/register ./Tests/*.spec.js",
    "build": "npm run babel && npm run uglify",
    "babel": "babel Src/Assembler.js --out-file Dist/Assembler.js",
    "uglify": "uglifyjs Dist/Assembler.js -o Dist/Assembler.min.js --comments -c -m",
    "jshint": "jshint Src/Assembler.js --config Build/.jshintrc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reduct/assembler.git"
  },
  "keywords": [
    "dom",
    "parser",
    "domParser",
    "assembler",
    "spe",
    "modules",
    "components"
  ],
  "author": "reduct",
  "contributors": [
