{
  "name": "@reduct/assembler",
  "version": "0.1.2",
  "description": "Parses a DOM Node for tags and executes the matching Constructor on each element.",
  "main": "Dist/Assembler.js",
  "scripts": {
    "test": "npm run build && npm run jshint && npm run karma",
    "build": "npm run babel && npm run uglify",
    "karma": "./node_modules/karma/bin/karma start Build/KarmaConfig.js",
    "babel": "./node_modules/.bin/babel Src/Assembler.js --out-file Dist/Assembler.js",
    "uglify": "node_modules/.bin/uglifyjs Dist/Assembler.js -o Dist/Assembler.min.js --comments -c -m",
    "jshint": "node_modules/.bin/jshint Src/Assembler.js --config Build/JsHintConfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reduct/assembler.git"
  },
  "keywords": [
    "dom",
    "parser",
    "domParser",
    "assembler",
    "spe",
    "modules",
    "components"
  ],
  "author": [
    {
      "name": "Tyll Wei√ü",
      "email": "inkdpixels@gmail.com",
      "url": "https://github.com/Inkdpixels/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reduct/assembler/issues"
  },
  "homepage": "https://github.com/reduct/assembler",
  "devDependencies": {
    "babel": "^5.8.3",
    "glob": "^4.3.5",
    "jasmine-core": "^2.2.0",
    "jshint": "^2.6.0",
    "karma": "^0.12.31",
    "karma-commonjs": "0.0.13",
    "karma-coverage": "^0.2.7",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "uglify-js": "^2.4.16"
  }
}
